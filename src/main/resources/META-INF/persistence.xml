<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="burritacoPU" transaction-type="JTA">
		<jta-data-source>jdbc/burritaco</jta-data-source>
		<class>model.Commune</class>
		<class>model.Congestion</class>
		<class>model.Keyword</class>
		<class>model.CommuneKeyword</class>
	</persistence-unit>
</persistence>
